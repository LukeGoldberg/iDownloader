<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.lashly.dao.CollectionRecordDao">

  <sql id="baseColumn">
    id, file_name, mongo_id, create_time, update_time
  </sql>

  <insert id="saveCollectionRecord" parameterType="org.lashly.domain.bo.CollectionRecordBo">
      insert into collection_record(file_name, mongo_id)
      values(#{fileName, jdbcType=VARCHAR}, #{mongoId, jdbcType=VARCHAR})
  </insert>

</mapper>